# LLM API 密钥配置

# OpenAI (GPT-4, GPT-4o)
OPENAI_API_KEY=sk-your-openai-api-key-here
# OpenAI 自定义端点（可选，用于代理或兼容服务）
OPENAI_BASE_URL=https://api.openai.com/v1

# Anthropic (Claude)
ANTHROPIC_API_KEY=sk-ant-your-anthropic-api-key-here
# Anthropic 自定义端点（可选）
ANTHROPIC_BASE_URL=https://api.anthropic.com

# Google (Gemini)
GOOGLE_API_KEY=your-google-api-key-here
# 或者使用 GOOGLE_GENAI_API_KEY
GOOGLE_GENAI_API_KEY=your-google-genai-api-key-here

# DeepSeek
DEEPSEEK_API_KEY=sk-your-deepseek-api-key-here
# DeepSeek 自定义端点（可选）
DEEPSEEK_BASE_URL=https://api.deepseek.com/v1

# 数据库配置
DATABASE_PATH=./data/debates.db

# 服务器配置
PORT=3000
NODE_ENV=development

# 日志级别
LOG_LEVEL=info

# LangSmith (可选 - 用于可观测性)
LANGCHAIN_TRACING_V2=true
LANGCHAIN_API_KEY=ls-your-langsmith-key-here
LANGCHAIN_PROJECT=debate-agents

# LangSmith 端点
LANGCHAIN_ENDPOINT=https://api.smith.langchain.com
